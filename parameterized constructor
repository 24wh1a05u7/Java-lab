public class ParameterizedConstructorExample {
    public static void main(String[] args) {
        
        Student s1 = new Student("Amit", 14);
        Student s2 = new Student("Varma", 15);

        s1.display();
        s2.display();
    }
}
class Student {
    String name;
    int age;

    
    Student(String n, int a) {
        name = n;
        age = a;
    }

    void display() {
        System.out.println("Name: " + name + ", Age: " + age);
    }
}
output
Name: Amit, Age: 14
Name: Varma, Age: 15

    
