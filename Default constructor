public class DefaultConstructorExample {
    public static void main(String[] args) {
        // Creating object, default constructor will be called
        Student s1 = new Student();
        s1.display();
    }
}
class Student {
    String name;
    int age;

    // Default constructor
    Student() {
        name = "Unknown";
        age = 0;
    }

    void display() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}
output
Name: Unknown
Age: 0
